# Korea Investment Securities Open Trading API Configuration
# Copy this file to .env and fill in your actual values

# Production environment
APP_KEY=your_prod_appkey_here
APP_SECRET=your_prod_appsecret_here

# Paper trading environment
PAPER_APP_KEY=your_paper_appkey_here
PAPER_APP_SECRET=your_paper_appsecret_here

# Account information
ACCT_STOCK=12345678
PROD_CODE=01

# Paper trading account (optional, uses ACCT_STOCK if not set)
PAPER_ACCT_STOCK=

# API base URLs
BASE_PROD=https://openapi.koreainvestment.com:9443
BASE_PAPER=https://openapivts.koreainvestment.com:29443

# User-Agent (required by KIS)
USER_AGENT=MyTradingApp/1.0
